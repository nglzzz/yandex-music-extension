match .* volume (class) {
    <div class="volume">
        @data-b = blockRoot(., "volume", true())
        if (class) {
            @class += " {class}"
        }
        <div class="volume__control">
            <div class="volume__track">
                <div class="volume__filled" style="height: 20px;"/>
            </div>
        </div>
        <div class="volume__holder">
        </div>
        <div class="volume__btn">
            <div class="volume__icon"/>
        </div>
    </div>
}
